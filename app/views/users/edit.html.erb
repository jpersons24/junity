<h1>Edit your information!</h1>

<%= form_for @user do |f| %>
   <%= f.label "Name:" %>
   <%= f.text_field :name %>
   <%= f.label "Username:" %>
   <%= f.text_field :username %>
   <%= f.label "Password:" %>
   <%= f.text_field :password %>
   <%= f.label "Age:" %>
   <%= f.number_field :age %>
   <%= f.submit "Confirm Changes" %>
<% end %>