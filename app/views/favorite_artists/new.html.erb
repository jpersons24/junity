<h1>Add artist to favorites list!</h1>

<%= form_for @favorite_artist do |f| %>
   <%= f.label "Who are you?" %>
   <%= f.collection_select :user_id, User.all, :id, :name %> <br>
   <%= f.label "Artist:" %>
   <%= f.collection_select :artist_id, Artist.all, :id, :name %> <br>
   <%= f.submit %>
<% end %>
